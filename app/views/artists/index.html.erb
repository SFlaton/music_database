<h1>Artists</h1>



<div class="row">
  <% @artists.each do |artist| %>
    <div class="col-lg-6 col-md-4">
        <div class="thumbnail">
          <%= image_tag(artist.image, size: "200x200") %>
          <div class="caption">
            <h3><%= artist.name %></h3>
            <p><%= link_to 'Show', artist, class: "btn btn-default" %></p>
          </div>
        </div>
    </div>
  <% end %>
</div>
